{
  "expo": {
    "name": "ReportRight",
    "slug": "ReportRight",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/ReportRightLarge2_512.png",
    "splash": {
      "image": "./assets/ReportRight.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c2122"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.AkConnects.ReportRight",
      "buildNumber": "4.0.0"
    },

    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/ReportRight_512.png",
        "backgroundColor": "#1c2122"
      },
      "package": "com.AkConnects.ReportRight"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
