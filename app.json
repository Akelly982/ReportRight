{
  "expo": {
    "name": "ReportRight",
    "slug": "ReportRight",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/ReportRightLarge_512.png",
    "splash": {
      "image": "./assets/ReportRight.png",
      "resizeMode": "contain",
      "backgroundColor": "#1c2122"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.AkConnects.ReportRight",
      "buildNumber": "2.0.0"
    },
    "android": {
      "permissions": ["CAMERA","READ_EXTERNAL_STORAGE","WRITE_EXTERNAL_STORAGE"],
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/ReportRight_512.png",
        "backgroundColor": "#1c2122"
      },
      "package": "com.AkConnects.ReportRight"
    },

    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
